FROM registry.access.redhat.com/redhat-openjdk-18/openjdk18-openshift:latest
MAINTAINER noconnor@redhat.com
ARG PATHFINDERBRANCH=none
ARG PATHFINDERCOMMITHASH=0
ARG PATHFINDERGITMSG=none
ARG PATHFINDERBUILDURL=none

LABEL PATHFINDERBRANCH=${PATHFINDERBRANCH}
LABEL PATHFINDERCOMMITHASH=${PATHFINDERCOMMITHASH}
LABEL PATHFINDERBUILDURL=${PATHFINDERBUILDURL}
LABEL PATHFINDERGITMSG=$PATHFINDERGITMSG
ADD pathfinder-server/target/pathfinder-server*-exec.jar /deployments/app.jar
